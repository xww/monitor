<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.spring.dao.HostDao" >
  <resultMap id="BaseResultMap" type="com.spring.entity.Host" >
    <id column="id" property="id" jdbcType="INTEGER" />
    <result column="private_mac" property="privateMac" jdbcType="VARCHAR" />
    <result column="host_name" property="hostName" jdbcType="VARCHAR" />
    <result column="sn" property="sn" jdbcType="VARCHAR" />
    <result column="project" property="project" jdbcType="VARCHAR" />
    <result column="idc" property="idc" jdbcType="VARCHAR" />
    <result column="machine_type" property="machineType" jdbcType="VARCHAR" />
    <result column="os_type" property="osType" jdbcType="VARCHAR" />
    <result column="alive" property="alive" jdbcType="INTEGER" />
    <result column="created_at" property="createdAt" jdbcType="TIMESTAMP" />
    <result column="updated_at" property="updatedAt" jdbcType="TIMESTAMP" />   
  </resultMap>
  
  <sql id="Base_Column_List" >
    id, host_id, cpu_util,created_at,updated_at
  </sql>
  
  <select id="getCpuSampleById" resultMap="BaseResultMap" parameterType="int" >
    select 
    <include refid="Base_Column_List" />
    from cpu
    where id = #{id,jdbcType=INTEGER}; 
  </select>
  
  <select id="getCpuSampleByTimeScope" resultMap="BaseResultMap" parameterType="java.util.Map" >
    select 
    <include refid="Base_Column_List" />
    from cpu
    where unix_timestamp(created_at) &gt;= unix_timestamp(#{start,jdbcType=VARCHAR}) and 
    unix_timestamp(created_at) &lt;= unix_timestamp(#{end,jdbcType=VARCHAR}); 
  </select>
  
  
  
</mapper>